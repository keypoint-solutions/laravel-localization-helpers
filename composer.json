{
  "name": "keypoint/laravel-localization-helpers",
  "description": "An artisan command package for easy translation management",
  "authors": [
    {
      "name": "Potsky"
    }
  ],
  "license": "GPL-3.0+",
  "require": {
    "illuminate/support": "^8.0|^9.0",
    "friendsofphp/php-cs-fixer": "^3.0"
  },
  "require-dev": {
    "orchestra/testbench": "^7.0",
    "phpunit/phpunit": "^9.0",
    "php-coveralls/php-coveralls": "dev-master"
  },
  "autoload": {
    "psr-0": {
      "Keypoint\\LaravelLocalizationHelpers\\": "src/"
    }
  },
  "minimum-stability": "stable",
  "extra": {
    "laravel": {
      "providers": [
        "Keypoint\\LaravelLocalizationHelpers\\LaravelLocalizationHelpersServiceProvider"
      ]
    }
  }
}
