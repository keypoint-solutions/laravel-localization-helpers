{
  "name": "schniper/laravel-localization-helpers",
  "description": "An artisan command package and methods for easy translation management. Derived from potsky's repo, adding PHP 8 / Laravel 8 & 9 support, updated MS translator support and other features",
  "authors": [
    {
      "name": "Potsky"
    },
    {
      "name": "Costin Bereveanu",
      "email": "cbereveanu@gmail.com"
    }
  ],
  "license": "GPL-3.0+",
  "require": {
    "illuminate/support": "^8.0|^9.0|^10.0",
    "friendsofphp/php-cs-fixer": "^3.0",
    "illuminate/console": "^v8.0|^9.0|^10.0",
    "illuminate/config": "^8.0|^9.0|^10.0"
  },
  "autoload": {
    "psr-4": {
      "Keypoint\\LaravelLocalizationHelpers\\": "src"
    }
  },
  "minimum-stability": "stable",
  "extra": {
    "laravel": {
      "providers": [
        "Keypoint\\LaravelLocalizationHelpers\\LaravelLocalizationHelpersServiceProvider"
      ]
    }
  },
  "require-dev": {
    "pestphp/pest": "^1.22|^2.0"
  },
  "config": {
    "allow-plugins": {
      "pestphp/pest-plugin": false
    }
  }
}
